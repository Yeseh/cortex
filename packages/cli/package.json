    "scripts": {
        "build": "tsc",
        "test": "bun test",
        "start": "bun run src/run.ts"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@yeseh/cortex-core": "workspace:*",
        "@yeseh/cortex-storage-fs": "workspace:*",
        "@commander-js/extra-typings": "^14.0.0",
        "@toon-format/toon": "^1.0.0",
        "commander": "^14.0.0"
    },
    "sideEffects": false
}
