{
    "name": "@yeseh/cortex-cli",
    "version": "0.5.0",
    "license": "MIT",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/Yeseh/cortex.git",
        "directory": "packages/cli"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com",
        "access": "public"
    },
    "bin": {
